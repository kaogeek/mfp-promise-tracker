{
  "name": "promise-tracker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:host": "nuxt --hostname 0.0.0.0 --port 80",
    "build": "nuxt build && nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --write .",
    "lint": "npm lint:js && npm lint:prettier",
    "lintfix": "prettier --write --list-different . && npm lint:js --fix",
    "prepare": "husky install",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "lint-staged": "lint-staged",
    "fetch-data": "ts-node -r dotenv/config scripts/monday-fetcher"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.**": "prettier --write --ignore-unknown"
  },
  "engines": {
    "node": "20 - 22"
  },
  "dependencies": {
    "@wevisdemo/ui": "^4.1.1",
    "core-js": "^3.37.1",
    "nuxt": "^2.18.1",
    "vue": "^2.7.16",
    "vue-plausible": "^1.3.2",
    "vue-server-renderer": "^2.7.16",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^4.47.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.8",
    "@nuxt/types": "^2.18.1",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.14.11",
    "@types/node-fetch": "^2.6.11",
    "@types/papaparse": "^5.3.14",
    "@vue/test-utils": "^1.3.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.5.0",
    "node-fetch": "^2.7.0",
    "postcss": "^8.4.39",
    "prettier": "^2.8.8",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^3.15.0",
    "vue-jest": "^3.0.7"
  }
}
